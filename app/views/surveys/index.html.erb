<header>My Surveys!</header>

<article class="controls">
  <%= link_to "Create New Survey", new_survey_path %>
</article>

<article class="surveys">
  <section class="titles">
    <% @surveys.load.each do |survey| %>
      <div class="survey-title">
        <%= link_to survey.title, survey %>
      </div>

    <% end %>
  </section>
</article>